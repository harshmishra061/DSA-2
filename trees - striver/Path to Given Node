bool find(TreeNode* node, int& target, vector<int>& res) {
    if(node == NULL) return false;
    
    res.push_back(node->val);
    
    if(node->val == target) {
        return true;
    }
    
    if(find(node->left, target, res)) {
        return true;
    }
    
    if(find(node->right, target, res)) {
        return true;
    }
    
    res.pop_back();
    
    return false;
} 
vector<int> Solution::solve(TreeNode* A, int B) {
    vector<int> res;
    find(A, B, res);
    return res;
}
