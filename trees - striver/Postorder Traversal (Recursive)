void postorder(TreeNode* node, vector<int>& res) {
    if(node == NULL) return;
    postorder(node->left, res);
    postorder(node->right, res);
    res.push_back(node->val);
}
public:
    vector<int> postorderTraversal(TreeNode* root) {
        vector<int> res;
        postorder(root, res);
        return res;
    }
};
