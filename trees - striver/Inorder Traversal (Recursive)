class Solution {
    void inorder(TreeNode* node, vector<int>& res) {
        if(node == NULL) return;
        inorder(node->left, res);
        res.push_back(node->val);
        inorder(node->right, res);
    }
public:
    vector<int> inorderTraversal(TreeNode* root) {
        vector<int> res;
        inorder(root, res);
        return res;
    }
};
